{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "build/**",
        "storybook-static/**"
      ]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "start": {},
    "check": {},
    "check:lint": {},
    "check:format": {},
    "check:typecheck": {},
    "check:style": {},
    "fix:format": {},
    "fix:lint": {},
    "storybook": {},
    "build:ui": {},
    "build:publish": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
