{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "start": {},
    "check:lint": {},
    "check:format": {},
    "check:lint-format": {},
    "check:style": {},
    "check:typecheck": {},
    "check:pa11y": {},
    "check": {},
    "test:unit": {},
    "bench": {},
    "coverage": {},
    "storybook": {},
    "build:publish": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
