{
  "extends": "stylelint-config-standard",
  "rules": {
    "selector-class-pattern": null,
    "import-notation": "string",
    "at-rule-no-deprecated": [
      true,
      {
        "ignoreAtRules": ["apply"]
      }
    ],
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["apply", "tailwind"]
      }
    ]
  }
}
