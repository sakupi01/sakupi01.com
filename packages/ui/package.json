{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "main": "src/index.ts",
  "exports": {
    ".": "./src/css/index.ts",
    "./index.css": "./src/css/index.css",
    "./sakupi01.com.css": "./src/css/sakupi01.com.css",
    "./sakupi01.com.markdown.css": "./src/css/sakupi01.com.markdown.css"
  },
  "type": "module",
  "scripts": {
    "check:format": "prettier --write .",
    "check:format:check": "prettier --check .",
    "check:lint": "eslint . --ext .js,.ts,.tsx",
    "check:typecheck": "tsc --noEmit",
    "check:lint-format": "pnpm check:lint && pnpm check:format",
    "check:style": "stylelint 'src/**/*.css'",
    "check": "pnpm run check:typecheck & pnpm check:lint & pnpm check:format:check & pnpm run check:style",
    "storybook": "storybook dev -p 6006 $VITE_STORYBOOK=true",
    "build:ui": "storybook build -o ./storybook-static && node transfer.js",
    "gen": "turbo gen react-component",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@storybook/addon-essentials": "8.3.0",
    "@storybook/addon-interactions": "8.3.0",
    "@storybook/addon-links": "8.3.0",
    "@storybook/addon-mdx-gfm": "8.3.0",
    "@storybook/addon-onboarding": "8.3.0",
    "@storybook/blocks": "8.3.0",
    "@storybook/manager-api": "8.3.0",
    "@storybook/react": "8.3.0",
    "@storybook/react-vite": "8.3.0",
    "@storybook/test": "8.3.0",
    "@storybook/theming": "8.3.0",
    "@turbo/gen": "^1.11.3",
    "@types/node": "^20.10.6",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "8.3.0",
    "stylelint": "^16.14.1",
    "typescript": "^5.3.3",
    "stylelint-config-standard": "^37.0.0"
  },
  "dependencies": {
    "blocks": "link:@storybook/addon-docs/blocks"
  }
}
